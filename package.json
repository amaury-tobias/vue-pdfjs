{
  "name": "vue-pdfjs-hook",
  "version": "0.0.1-beta.1",
  "license": "MIT",
  "author": "Amaury Tobias <amaury.tobiasqr@gmail.com> (https://amaurytq.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/amaury-tobias/vue-pdfjs-hook.git"
  },
  "description": "Vue hook to use easy pdfjs-dist and render a PDF Document in SVG mode",
  "keywords": [
    "vue",
    "pdfjs",
    "pdf"
  ],
  "main": "./dist/vue-pdfjs-hook.umd.js",
  "module": "./dist/vue-pdfjs-hook.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-pdfjs-hook.es.js",
      "require": "./dist/vue-pdfjs-hook.umd.js"
    }
  },
  "types": "./main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "serve": "vite preview",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "pdfjs-dist": "^2.7.570",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.52",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "pdfjs-dist": "^2.7.570",
    "vue": "^3.0.5"
  }
}
