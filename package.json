{
  "name": "vue-pdfjs-hook",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Amaury Tobias <amaury.tobiasqr@gmail.com> (https://amaurytq.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/amaury-tobias/vue-pdfjs-hook.git"
  },
  "description": "Vue hook to use easy pdfjs-dist and render a PDF Document in SVG mode",
  "keywords": [
    "vue",
    "pdfjs",
    "pdf"
  ],
  "main": "./dist/vue-pdfjs-hook.umd.js",
  "module": "./dist/vue-pdfjs-hook.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-pdfjs-hook.es.js",
      "require": "./dist/vue-pdfjs-hook.umd.js"
    }
  },
  "types": "./main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "yarn build",
    "lint:script": "eslint --ext .ts,.vue ."
  },
  "devDependencies": {
    "@attq/eslint-config-vue": "^0.1.2",
    "@vitejs/plugin-vue": "^1.9.2",
    "eslint": "^7.32.0",
    "pdfjs-dist": "^2.9.359",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue": "^3.2.19"
  },
  "peerDependencies": {
    "pdfjs-dist": "^2.7.570",
    "vue": "^3.0.5"
  }
}
